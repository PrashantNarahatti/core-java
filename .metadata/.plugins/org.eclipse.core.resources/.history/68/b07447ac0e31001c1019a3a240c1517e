package com.xworkz.array;

public class ArrayStarter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     
		String pacchu="KA 29 9275";
		String prashant="KA 29 5528";
		String pranu="KA 29 0864";
		
		String[] vehicleNos=new String[4];
		vehicleNos[3]=pacchu;
		vehicleNos[1]=prashant;
		vehicleNos[0]=pranu;
		
		for(int i=0;i<vehicleNos.length; i++)
		{
			System.out.println(i);
			String temp=vehicleNos[i];
			if(temp!=null)
			{
				temp=temp.toLowerCase();
				System.out.println(temp);
			}
			else
			{
				System.out.println(temp);
			}
		}
		
		//pan card
		
		String parasu="BFHPN7202Q";
		String raju="BFHPN8531E";
		String anu="BHEUG8421K";
		String vinayak="BHEPT8321M";
		String abhi="BJUGK8421K";
		String jyoti="BHEUG3581S";
		String rohit="BDFTH2421K";
		String muttu="EHEUG3585G";
		String umesh="FGJKU6426L";
		String ramu="FGSKU3326H";
		
		String[] panCardNos=new String[9];
		panCardNos[3]=parasu;
		panCardNos[1]=raju;
		panCardNos[0]=anu;
		panCardNos[2]=vinayak;
		panCardNos[5]=abhi;
		panCardNos[7]=jyoti;
		panCardNos[8]=rohit;
		panCardNos[4]=muttu;
		panCardNos[6]=umesh;
		panCardNos[9]=ramu;
		
		
		
		for(int pan=0;pan<panCardNos.length; pan++)
		{
			System.out.println(pan);
			String temp1=panCardNos[pan];
			if(temp1!=null)
			{
				temp1=temp1.toLowerCase();
				System.out.println(temp1);
			}
			else
			{
				System.out.println(temp1);
		}
	}

}
}