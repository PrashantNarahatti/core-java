package com.xworkz.BikeStarter;

public class MarketStarter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String marketname="D-Mart";
		String sub=marketname.substring(0,3);
		System.out.println(sub);
		
		String Bgk="Bgk";
		String market=new String("market");
		if(Bgk==market)
		{
			System.out.println("String are pointing to same memory");
		}
		else {
			System.out.println("String are pointing to different memory");
		}
		String market1="market";
		if(market==market1) {
			System.out.println("String are pointing to same memory");
			
		}
		else
		{
			System.out.println("String are pointing to different memory");
		}
		
		market1="Apple";
		System.out.println(market1);
		
		market1="Banana";
		System.out.println(market1);
		
		market1=market;
		System.out.println(market1);       
		
		if(market1==market)
		{
			System.out.println("market and market1 pointing to same memory");
		}
		else
		{
			System.out.println("market and market1 pointing to different memory");	
		}
	}

}
