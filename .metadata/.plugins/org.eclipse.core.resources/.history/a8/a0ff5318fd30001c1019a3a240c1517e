package com.xworkz.array;

public class GroupNumberStarter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     //aadhar
		
		long aadhar1=123456789533L;
		long aadhar2=287643581572L;
		long aadhar3=587615867915L;
		long aadhar4=573548628758L;
		long aadhar5=820040670405L;
		long aadhar6=638428769124L;
		long aadhar7=672800054680L;
		long aadhar8=800500530803L;
		long aadhar9=550560084912L;
		long aadhar10=75550500571L;
		
long[] aadharNos= {aadhar1,aadhar2,aadhar3,aadhar4,aadhar5,aadhar6,aadhar7,aadhar8,aadhar9,aadhar10};
    
	for(int num=0;num<aadharNos.length;num++)
	{
		System.out.println(num);
		long number=aadharNos[num];
		System.out.println(number);
		
	}
	
	//bankaccount
	
	long bankaccount1=123456789533L;
	long bankaccount2=287643581572L;
	long bankaccount3=587615867915L;
	long bankaccount4=573548628758L;
	long bankaccount5=820040670405L;
	long bankaccount6=638428769124L;
	long bankaccount7=672800054680L;
	long bankaccount8=800500530803L;
	long bankaccount9=550560084912L;
	long bankaccount10=75550500571L;
	
long[] bankaccountNos= {bankaccount1,bankaccount2,bankaccount3,bankaccount4,bankaccount5,bankaccount6,bankaccount7,bankaccount8,bankaccount9,bankaccount10};

for(int bank=0;bank<bankaccountNos.length;bank++)
{
	System.out.println(bank);
	long number=aadharNos[bank];
	System.out.println(number);
	
}
    //pincode

  double pincode1=587101d;
  double pincode2=587102d;
  double pincode3=587103d;
  double pincode4=587111d;
  double pincode5=587126d;
  double pincode6=587128d;
  double pincode7=587132d;
  double pincode8=587136d;
  double pincode9=587141d;
  double pincode10=582116d;
  
  double[] pincodeNos= {pincode1,pincode2,pincode3,pincode4,pincode5,pincode6,pincode7,pincode8,pincode9,pincode10};

  for(int pin=0;pin<pincodeNos.length;pin++)
  {
  	System.out.println(pin);
  	long number=aadharNos[pin];
  	System.out.println(number);
  	
  }
	}

}
